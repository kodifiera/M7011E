{
	"plugins": ["jest"],
	"extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:jest/recommended"],
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"rules": {
		"no-console": [2, { "allow": ["info", "warn", "error"] }]
	}
}
